DECLARE FUNCTION IsSafe! (levels() AS INTEGER, count AS INTEGER)

CONST NumLines = 1000
CONST MaxLevels = 10
CONST Part2 = 1

DIM lines(1 TO NumLines, 1 TO MaxLevels) AS INTEGER
DIM sizes(1 TO NumLines) AS INTEGER

FOR i = 1 TO NumLines
  READ sizes(i)
 
  FOR j = 1 TO sizes(i)
    READ lines(i, j)
  NEXT j
NEXT i

DIM result AS INTEGER
DIM SHARED tmp(1 TO MaxLevels) AS INTEGER
result = 0

FOR i = 1 TO NumLines
  FOR j = 1 TO sizes(i)
    tmp(j) = lines(i, j)
  NEXT j

  IF IsSafe(tmp(), sizes(i)) THEN
    result = result + 1
  ELSEIF Part2 THEN
    FOR skp = 1 TO sizes(i)
      DIM index AS INTEGER
      index = 1
      FOR j = 1 TO sizes(i)
        IF j <> skp THEN
          tmp(index) = lines(i, j)
          index = index + 1
        END IF
      NEXT j
      IF IsSafe(tmp(), sizes(i) - 1) THEN
        result = result + 1
        EXIT FOR
      END IF
    NEXT skp
  END IF
NEXT i

PRINT result

