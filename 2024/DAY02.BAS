FUNCTION IsSafe (levels() AS INTEGER, count AS INTEGER)
  DIM score AS INTEGER
  score = 0

  FOR i = 2 TO count
    SELECT CASE levels(i) - levels(i - 1)
      CASE -3 TO -1
        score = score - 1
      CASE 1 TO 3
        score = score + 1
    END SELECT
  NEXT i

  IsSafe = (ABS(score) = count - 1)
END FUNCTION

